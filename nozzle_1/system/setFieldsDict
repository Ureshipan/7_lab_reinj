FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues 
( 
    volScalarFieldValue p 8000
);

regions         
( 
  boxToCell 
  { 
   box (-1.1850024052703034 0 -1) (0 0.1955 1);
      fieldValues 
      ( 
            volScalarFieldValue p 200000
      ) ; 
    } 
);


